{
    "manifest_version": 2,
    "author": "MatevzFa",
    "homepage_url": "https://github.com/MatevzFa/osrs-polls-extension",
    "name": "Enhanced Old School RuneScape Poll Results",
    "version": "0.0.6",
    "description": "It makes official poll result pages useful.",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://services.runescape.com/*poll*/oldschool/results.ws?id=*"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "jquery.min.js",
                "script.js"
            ]
        }
    ]
}
